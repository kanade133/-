<Window x:Class="WpfApplication4.TestWindow.PrintPDF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication4.TestWindow"
        mc:Ignorable="d"
        Title="PrintPDF" Height="450" Width="800">
    <Grid x:Name="gridRoot">
        <StackPanel>
            <Button Margin="5" Content="PDF" Click="Button_Click"/>
            <Button Margin="5" Content="PNG" Click="Button_Click_1"/>
            <Canvas x:Name="canvas">
                <Line X1="100" Y1="30" X2=" 400" Y2=" 30" Stroke="Black"/>
                <Line X1="100" Y1="60" X2=" 400" Y2=" 30" Stroke="Black"/>
                <Line X1="100" Y1="60" X2=" 400" Y2=" 60" Stroke="Black"/>
            </Canvas>
        </StackPanel>
    </Grid>
</Window>
